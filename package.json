{
  "name": "jinge-material",
  "version": "0.0.0",
  "description": "A material design ui library implemented with jinge",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run dev-site",
    "dev-site": "node ./script/site-dev.js",
    "build-site": "rm -rf ./docs && node ./script/site-build.js",
    "build": "rm -rf ./dist && node ./script/build.js",
    "lint": "npm run stylelint && npm run eslint",
    "stylelint": "stylelint '**/*.scss' --syntax scss",
    "eslint": "eslint .",
    "fixstylelint": "stylelint '**/*.scss' --syntax scss --fix",
    "fixeslint": "eslint --fix .",
    "update-icons": "node ./script/update_icons.js",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuhangGe/jinge-material.git"
  },
  "keywords": [
    "material",
    "ui",
    "library",
    "jinge"
  ],
  "author": "Yuhang Ge <abeyuhang@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YuhangGe/jinge-material/issues"
  },
  "homepage": "https://github.com/YuhangGe/jinge-material#readme",
  "peerDependencies": {
    "jinge": "^1.0.6",
    "jinge-ui-router": "^1.0.3"
  },
  "devDependencies": {
    "clean-css": "^4.2.1",
    "clipboard": "^2.0.4",
    "copy-webpack-plugin": "^5.1.0",
    "css-loader": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^8.1.0",
    "highlight.js": "^9.16.2",
    "lodash-es": "^4.17.15",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "precommit-hook": "^3.0.0",
    "sass-loader": "^8.0.0",
    "single-line-log": "^1.1.2",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.13.0",
    "terser": "^4.4.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "popper.js": "^1.16.0"
  }
}
