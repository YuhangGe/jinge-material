{
  "name": "jinge-material",
  "version": "0.0.0",
  "description": "A material design ui library implemented with jinge",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config ./site/webpack.config.js",
    "dev-en": "BUILD_LOCALE=en npm run dev",
    "build": "npm run generate-i18n-csv && npm run build-prod && BUILD_LOCALE=en npm run build-prod",
    "build-prod": "PRODUCTION=true npm run build-dev",
    "build-dev": "webpack --config ./site/webpack.config.js",
    "watch": "WATCH=true npm run build-dev",
    "generate-i18n-csv": "GENERATE_I18N_CSV=true npm run build-dev",
    "lint": "npm run stylelint && npm run eslint",
    "stylelint": "stylelint '**/*.scss' --syntax scss",
    "eslint": "eslint .",
    "fixstylelint": "stylelint '**/*.scss' --syntax scss --fix",
    "fixeslint": "eslint --fix .",
    "update-icons": "node ./script/update_icons.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuhangGe/jinge-material.git"
  },
  "keywords": [
    "material",
    "ui",
    "library",
    "jinge"
  ],
  "author": "Yuhang Ge <abeyuhang@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YuhangGe/jinge-material/issues"
  },
  "homepage": "https://github.com/YuhangGe/jinge-material#readme",
  "peerDependencies": {
    "jinge": "^1.0.6",
    "jinge-ui-router": "^1.0.3"
  },
  "devDependencies": {
    "clipboard": "^2.0.4",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^8.1.0",
    "highlight.js": "^9.15.10",
    "https-proxy-agent": "^2.2.2",
    "node-sass": "^4.12.0",
    "popper.js": "^1.15.0",
    "precommit-hook": "^3.0.0",
    "sass-loader": "^8.0.0",
    "single-line-log": "^1.1.2",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.10.1",
    "terser": "^4.3.1",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.1"
  },
  "pre-commit": [
    "lint"
  ]
}
